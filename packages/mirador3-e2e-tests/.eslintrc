{
  "env": {
    "jest/globals": true
  },
  "extends": "airbnb",
  "globals": {
    "page": true,
    "document": true
  },
  "plugins": ["jest"],
  "rules": {
    "no-console": "off",
    "global-require": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "require-jsdoc": 0,
    "react/prefer-stateless-function": "off"
  }
}
